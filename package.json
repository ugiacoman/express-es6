{
  "name": "express-es6",
  "version": "1.0.0",
  "description": "Express + ES6 + Standard Linting + Istanbul + Debug + Nodemon + Jest + PostgraphQL",
  "main": "src/index.js",
  "repository": "git@github.com:ugiacoman/express-es6.git",
  "author": "Ulises Giacoman",
  "license": "MIT",
  "scripts": {
    "prestart": "yarn build",
    "start": "node dist/index.js",
    "dev": "nodemon src/index.js --exec \"node -r babel-register\"",
    "sweep": "rimraf dist",
    "build": "yarn sweep clean && mkdir -p dist && babel src -s -D -d dist"
  },
  "dependencies": {
    "body-parser": "^1.17.1",
    "express": "^4.15.2",
    "morgan": "^1.8.1",
    "pug": "^2.0.0-beta.12"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.4.0",
    "babel-register": "^6.24.1",
    "nodemon": "^1.11.0",
    "rimraf": "^2.6.1",
    "standard": "^10.0.2"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ]
  }
}
